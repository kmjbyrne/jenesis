{
    "name": "@snapatoms/core",
    "version": "0.0.4",
    "description": "Complete Rapid Development Library. Useful reusable front end components for projects",
    "main": "./dist/app.bundle.js",
    "scripts": {
        "start": "run build",
        "build": "./node_modules/.bin/webpack --progress --profile --colors --mode production --config webpack/prod.config.js",
        "build:watch": "./node_modules/.bin/webpack --progress --colors --mode production --config webpack/prod.config.js --watch",
        "start:dev": "./node_modules/.bin/webpack-dev-server --progress --colors --mode development --config webpack/dev.config.js",
        "test": "karma start ./karma.conf.js --singleRun true",
        "test-watch": "karma start ./karma.conf.js --singleRun false"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kmjbyrne/snapatoms.git"
    },
    "keywords": [
        "js",
        "css",
        "style",
        "build",
        "bootstrap"
    ],
    "author": "Keith Byrne",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kmjbyrne/snapatoms/issues"
    },
    "files": [
        "/dist"
    ],
    "homepage": "https://github.com/kmjbyrne/snapatoms#readme",
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-env": "^7.5.5",
        "@babel/preset-es2017": "^7.0.0-beta.53",
        "@fortawesome/fontawesome-free": "^5.8.1",
        "autoprefixer": "^9.6.1",
        "babel": "^6.23.0",
        "babel-loader": "^8.0.0-beta.6",
        "babel-preset-es2015": "^6.24.1",
        "base64-inline-loader": "^1.1.1",
        "clean-webpack-plugin": "^3.0.0",
        "create-file-webpack": "^1.0.2",
        "css-loader": "^3.1.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.0.6",
        "manifest-revision-webpack-plugin": "^0.4.1",
        "mini-css-extract-plugin": "^0.8.0",
        "node-sass": "^7.0.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss-loader": "^2.1.6",
        "precss": "^2.0.0",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "terser-webpack-plugin": "^2.0.1",
        "ts-loader": "^6.0.4",
        "typescript": "^3.5.3",
        "webpack": "^4.38.0",
        "webpack-cli": "^3.3.6",
        "webpack-dev-server": "^3.7.2"
    },
    "dependencies": {
        "prismjs": "^1.19.0"
    }
}
